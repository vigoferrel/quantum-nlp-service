    "client": "node vigoleonrocks-cloud-client.js",
    "test": "python test-suite.py",
    "benchmark": "python benchmark-tests.py",
    "stress": "python stress-tests.py",
    "docker:build": "docker build -f Dockerfile.cloud -t vigoleonrocks-cloud .",
    "docker:run": "docker-compose -f docker-compose.cloud.yml up -d",
