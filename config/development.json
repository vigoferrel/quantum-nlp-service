{
  "environment": "development",
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "vigoleonrocks_dev",
    "user": "postgres",
    "password": "",
    "pool_size": 5,
    "max_overflow": 10
  },
  "api": {
    "host": "localhost",
    "port": 5001,
    "debug": true,
    "cors_origins": ["http://localhost:3000", "http://127.0.0.1:3000"],
    "rate_limit": 1000,
    "timeout": 30
  },
  "mcp": {
    "host": "localhost",
    "port": 5002,
    "protocol_version": "2024-11-05",
    "max_connections": 50,
    "heartbeat_interval": 30
  },
  "monitoring": {
    "enabled": true,
    "metrics_port": 9090,
    "health_check_interval": 30,
    "alert_thresholds": {
      "cpu_percent": 80,
      "memory_percent": 85,
      "disk_percent": 90
    }
  },
  "notifications": {
    "email_enabled": false,
    "smtp_host": "smtp.gmail.com",
    "smtp_port": 587,
    "smtp_user": "",
    "smtp_password": "",
    "webhook_urls": []
  },
  "security": {
    "secret_key": "dev_secret_key_change_in_production",
    "jwt_expiration": 3600,
    "api_key_expiration": 86400,
    "max_login_attempts": 5,
    "password_min_length": 8
  },
  "vigoleonrocks": {
    "model_name": "vigoleonrocks_optimized",
    "max_tokens": 4096,
    "temperature": 0.7,
    "top_p": 0.9,
    "quantum_enabled": true,
    "memory_26d_enabled": true
  }
}
